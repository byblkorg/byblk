{
  "name": "@gcmp/geoffrey",
  "version": "0.0.7",
  "description": "General purposed stack.",
  "main": "dist/geoffrey/src/index.js",
  "types": "dist/geoffrey/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "push": "tsc && npm publish --access public",
